{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./public/",
    "sourceMap": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ScriptHost"
    ],
    "downlevelIteration": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strict": true
  }
}
